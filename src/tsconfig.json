{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es6",
    "module": "es2015",
    "rootDir": "./",
    "outDir": "../packages",
    "baseUrl": ".",
    "paths": {
      "@ecsy-babylon": [
        "./index"
      ]
    },
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
  },
}